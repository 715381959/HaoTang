@using GMS.Framework.Contract
@using GMS.Framework.Web.Controls
@using GMS.Basedata.Contract
@using GMS.Web
@model Material
@{
    Layout = "~/Views/Shared/_Layout.Edit.cshtml";
}
@section PageSpecificStyleSheetIncludes{
}
@section PageSpecificJavascriptIncludes{
    <script type="text/javascript">
   </script>
}
@section MainContent{
<div class="portlet-body form-horizontal form-bordered form-row-stripped">
    <div class="row-fluid"> 
            <div class="control-group">
                <label class="control-label"><span class="required">分类</span>编码：</label>
                <div class="controls">
                @Html.TextBoxFor(m => m.MaterialCode, new { @class = "m-wrap" })
                <span class="help-inline">@Html.ValidationMessageFor(m => m.MaterialCode)</span>
                </div>
            </div>

            <div class="control-group">
                <label class="control-label">分类名称：</label>
                <div class="controls">
                @Html.TextBoxFor(m => m.MaterialName, new { @class = "m-wrap large" })
                <span class="help-inline">@Html.ValidationMessageFor(m => m.MaterialName)</span>
                </div>
            </div>
            
    </div>
</div>
}

