@using GMS.Framework.Contract
@using GMS.Framework.Utility
@using GMS.Framework.Web.Controls
@using GMS.Project.Contract

@model MaterialCost
@{
    Layout = "~/Views/Shared/_Layout.Edit.cshtml";
}
@section PageSpecificStyleSheetIncludes{
}
@section PageSpecificJavascriptIncludes{
    <script type="text/javascript">
    </script>
}
@section MainContent{
    <div class="portlet-body form-horizontal form-bordered form-row-stripped">
        <div class="row-fluid">
            <div class="control-group">
                <label class="control-label"><span class="required">*</span>项目名称：</label>
                <div class="controls">
                    @Html.TextBoxFor(m => m.ProjectName, new { @class = "m-wrap" })
                    <span class="help-inline">@Html.ValidationMessageFor(m => m.ProjectName)</span>
                </div>
            </div>

            <div class="control-group">
                <label class="control-label">钢材：</label>
                <div class="controls">
                    @Html.TextBoxFor(m => m.MSteel, new { @class = "m-wrap small" })
                    <span class="help-inline">@Html.ValidationMessageFor(m => m.MSteel)</span>
                </div>
            </div>

            <div class="control-group">
                <label class="control-label">朔料管件及配件：</label>
                <div class="controls">
                    @Html.TextBoxFor(m => m.MHose, new { @class = "m-wrap small" })
                    <span class="help-inline">@Html.ValidationMessageFor(m => m.MHose)</span>
                </div>
            </div>
            <div class="control-group">
                <label class="control-label">水泥及制品：</label>
                <div class="controls">
                    @Html.TextBoxFor(m => m.MCement, new { @class = "m-wrap large" })
                    <span class="help-inline">@Html.ValidationMessageFor(m => m.MCement)</span>
                </div>
            </div> 
            <div class="control-group">
                <label class="control-label">灰油：</label>
                <div class="controls">
                    @Html.TextBoxFor(m => m.MGrayOil, new { @class = "m-wrap large" })
                    <span class="help-inline">@Html.ValidationMessageFor(m => m.MGrayOil)</span>
                </div>
            </div>
           
            <div class="control-group">
                <label class="control-label">电线电缆：</label>
                <div class="controls">
                    @Html.TextBoxFor(m => m.MWire, new { @class = "m-wrap large" })
                    <span class="help-inline">@Html.ValidationMessageFor(m => m.MWire)</span>
                </div>
            </div>
            <div class="control-group">
                <label class="control-label">木竹及制品：</label>
                <div class="controls">
                    @Html.TextBoxFor(m => m.MWood, new { @class = "m-wrap large" })
                    <span class="help-inline">@Html.ValidationMessageFor(m => m.MWood)</span>
                </div>
            </div>
            <div class="control-group">
                <label class="control-label">瓷砖类：</label>
                <div class="controls">
                    @Html.TextBoxFor(m => m.MCeramicTile, new { @class = "m-wrap large" })
                    <span class="help-inline">@Html.ValidationMessageFor(m => m.MCeramicTile)</span>
                </div>
            </div>
            <div class="control-group">
                <label class="control-label">门窗型材及配件类：</label>
                <div class="controls">
                    @Html.TextBoxFor(m => m.MWindowsDoors, new { @class = "m-wrap large" })
                    <span class="help-inline">@Html.ValidationMessageFor(m => m.MWindowsDoors)</span>
                </div>
            </div>

              <div class="control-group">
                <label class="control-label">玻璃及配件：</label>
                <div class="controls">
                    @Html.TextBoxFor(m => m.MGalss, new { @class = "m-wrap large" })
                    <span class="help-inline">@Html.ValidationMessageFor(m => m.MGalss)</span>
                </div>
            </div>

              <div class="control-group">
                <label class="control-label">零星五金类：</label>
                <div class="controls">
                    @Html.TextBoxFor(m => m.MHardware, new { @class = "m-wrap large" })
                    <span class="help-inline">@Html.ValidationMessageFor(m => m.MHardware)</span>
                </div>
            </div>

              <div class="control-group">
                <label class="control-label">电气及配件：</label>
                <div class="controls">
                    @Html.TextBoxFor(m => m.MElectric, new { @class = "m-wrap large" })
                    <span class="help-inline">@Html.ValidationMessageFor(m => m.MElectric)</span>
                </div>
            </div>

              <div class="control-group">
                <label class="control-label">混凝土：</label>
                <div class="controls">
                    @Html.TextBoxFor(m => m.MConcrete, new { @class = "m-wrap large" })
                    <span class="help-inline">@Html.ValidationMessageFor(m => m.MConcrete)</span>
                </div>
            </div>
              <div class="control-group">
                <label class="control-label">砖：</label>
                <div class="controls">
                    @Html.TextBoxFor(m => m.MBrick, new { @class = "m-wrap large" })
                    <span class="help-inline">@Html.ValidationMessageFor(m => m.MBrick)</span>
                </div>
            </div>

              <div class="control-group">
                <label class="control-label">模板：</label>
                <div class="controls">
                    @Html.TextBoxFor(m => m.MTemplate, new { @class = "m-wrap large" })
                    <span class="help-inline">@Html.ValidationMessageFor(m => m.MTemplate)</span>
                </div>
            </div>

              <div class="control-group">
                <label class="control-label">铝材：</label>
                <div class="controls">
                    @Html.TextBoxFor(m => m.MAluminum, new { @class = "m-wrap" })
                    <span class="help-inline">@Html.ValidationMessageFor(m => m.MAluminum)</span>
                </div>
            </div>

              <div class="control-group">
                <label class="control-label">材料检测费：</label>
                <div class="controls">
                    @Html.TextBoxFor(m => m.MTest, new { @class = "m-wrap" })
                    <span class="help-inline">@Html.ValidationMessageFor(m => m.MTest)</span>
                </div>
            </div>

              <div class="control-group">
                <label class="control-label">消防、通风、排烟：</label>
                <div class="controls">
                    @Html.TextBoxFor(m => m.MFireAirSmoke, new { @class = "m-wrap" })
                    <span class="help-inline">@Html.ValidationMessageFor(m => m.MFireAirSmoke)</span>
                </div>
            </div>

              <div class="control-group">
                <label class="control-label">周转材料租赁：</label>
                <div class="controls">
                    @Html.TextBoxFor(m => m.MTurnoverRent, new { @class = "m-wrap " })
                    <span class="help-inline">@Html.ValidationMessageFor(m => m.MTurnoverRent)</span>
                </div>
            </div>

              <div class="control-group">
                <label class="control-label">周转材料摊销：</label>
                <div class="controls">
                    @Html.TextBoxFor(m => m.MTurnovers, new { @class = "m-wrap" })
                    <span class="help-inline">@Html.ValidationMessageFor(m => m.MTurnovers)</span>
                </div>
            </div>

              <div class="control-group">
                <label class="control-label">其他材料费用：</label>
                <div class="controls">
                    @Html.TextBoxFor(m => m.MOther, new { @class = "m-wrap" })
                    <span class="help-inline">@Html.ValidationMessageFor(m => m.MOther)</span>
                </div>
            </div>

             
          
           
        </div>
    </div>
}

